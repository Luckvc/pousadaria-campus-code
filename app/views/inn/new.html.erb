<h1> Cadastrar Pousada </h1>
<h3> Dados da Empresa </h3>
<%= form_with(model: @inn, url: user_inn_index_path) do |f| %>
  <div>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= f.label :company_name %>
      <%= f.text_field :company_name %>
    </div>
    <div>
      <%= f.label :cnpj %>
      <%= f.text_field :cnpj %>
    </div>
    <div>
      <%= f.label :phone %>
      <%= f.text_field :phone %>
    </div>
    <div>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>
  </div>
  <div>
  <h3> EndereÃ§o da Pousada </h3>
    <%= form_with(model: @address, url: address_index_path) do |f| %>
      <div>
        <%= f.label :street %>
        <%= f.text_field :street %>
      </div>
      <div>
        <%= f.label :number %>
        <%= f.text_field :number %>
      </div>
      <div>
        <%= f.label :neighborhood %>
        <%= f.text_field :neighborhood %>
      </div>
      <div>
        <%= f.label :city %>
        <%= f.text_field :city %>
      </div>
      <div>
        <%= f.label :state %>
        <%= f.text_field :state %>
      </div>
      <div>
        <%= f.label :cep %>
        <%= f.text_field :cep %>
      </div>
    <% end %>
  </div><br>
  <div>
    <%= f.submit 'Enviar' %>
  </div>
<% end %>


